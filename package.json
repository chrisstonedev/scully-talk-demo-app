{
  "name": "dev.chrisstone.web",
  "displayName": "Chris Stone",
  "version": "1.0.0",
  "description": "Web app portfolio for Chris Stone.",
  "author": "Chris Stone",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --browsers=ChromeHeadless",
    "test:i": "ng test --browsers=Chrome",
    "deploy": "ng deploy",
    "lint": "ng lint",
    "e2e": "cypress run",
    "e2e:i": "cypress open",
    "favicon": "real-favicon check-for-update --fail-on-update faviconData.json",
    "favicon:generate": "real-favicon generate faviconDescription.json faviconData.json src",
    "scully": "npx scully --",
    "scully:serve": "ng build && npx scully -- && npx scully killServer --no-warning && npx scully serve --",
    "scully:build": "ng build && npx scully -- --scanRoutes"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.0",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/forms": "^13.0.0",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "@scullyio/init": "^2.0.1",
    "@scullyio/ng-lib": "^2.0.0",
    "@scullyio/scully": "^2.0.0",
    "bootstrap": "^5.1.3",
    "rxjs": "^7.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.1",
    "@angular-eslint/builder": "^13.0.0-alpha.0",
    "@angular-eslint/eslint-plugin": "^13.0.0-alpha.0",
    "@angular-eslint/eslint-plugin-template": "^13.0.0-alpha.0",
    "@angular-eslint/schematics": "^13.0.0-alpha.0",
    "@angular-eslint/template-parser": "^13.0.0-alpha.0",
    "@angular/cli": "^13.0.1",
    "@angular/compiler-cli": "^13.0.0",
    "@angular/language-service": "^13.0.0",
    "@types/jasmine": "^3.10.2",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "cli-real-favicon": "^0.0.8",
    "cypress": "^8.7.0",
    "eslint": "^8.1.0",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.7",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-teamcity-reporter": "^1.1.0",
    "puppeteer": "^11.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~4.4.4"
  }
}
